id: {{id}}
kind: command
category: {{category}}
{{#if domain}}
domain: {{domain}}
{{/if}}
summary: "{{summary}}"
{{#if tags}}
tags:
{{#each tags}}
  - {{this}}
{{/each}}
{{/if}}

defaults:
  preferred_models:
    - claude/sonnet

context_usage:
  as_system: false
  as_user: true
  as_overlay: false

tools: []

inputs:
  - name: task_description
    type: string
    required: true
    description: "Description of the task to perform"

versions: []

